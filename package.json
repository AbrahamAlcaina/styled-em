{
  "name": "styled-elm",
  "version": "1.0.0",
  "description": "This project is bootstrapped with [Create Elm App](https://github.com/halfzebra/create-elm-app).",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "start": "node scripts/start.js",
    "package": "elm-package",
    "make": "elm-make",
    "repl": "elm-repl",
    "reactor": "elm-reactor",
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "./node_modules/.bin/elm-css --pathToMake ./node_modules/.bin/elm-make -o ./src ./src/Stylesheets.elm",
    "i18n-switch-es": "./node_modules/.bin/elm-i18n-switch -l es",
    "i18n-switch-en": "./node_modules/.bin/elm-i18n-switch -l en",
    "i18n-export-es": "./node_modules/.bin/elm-i18n-generator --format CSV --language es --root translation --export",
    "i18n-export-en": "./node_modules/.bin/elm-i18n-generator --format CSV --language en --root translation --export",
    "i18n-import-es": "./node_modules/.bin/elm-i18n-generator --format CSV --language es --root false --importOutput translation --import export.csv && mv",
    "i18n-import-en": "./node_modules/.bin/elm-i18n-generator --format CSV --language en --root false --importOutput translation --import export.csv"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "chalk": "1.1.3",
    "clean-webpack-plugin": "^0.1.16",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "elm": "^0.18.0",
    "elm-css": "^0.6.1",
    "elm-hot-loader": "0.5.4",
    "elm-i18n": "^1.0.0",
    "elm-test": "^0.18.4",
    "elm-webpack-loader": "^4.3.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "http-proxy-middleware": "^0.17.3",
    "minimist": "1.2.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "react-dev-utils": "^3.0.0",
    "react-error-overlay": "^1.0.7",
    "sass-loader": "^6.0.6",
    "string-replace-loader": "^1.2.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
